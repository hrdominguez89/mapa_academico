{% extends 'base.html.twig' %}

{% block title %}
	{{ title }}
{% endblock %}


{% block body %}
	{# Carga modal en caso de haber mensajes para mostrar #}


	<div
		class="container-fluid">

		<!-- ========== Top Bar Start ========== -->
		{% include 'secure/navbar.html.twig' %}
		<!-- Top Bar End -->
	</div>
	<div class="container">
		{% if breadcrumbs is defined %}
			{% include 'secure/breadcrumbs.html.twig' %}
		{% endif %}
		{% for message in app.flashes('message') %}
			{% include 'secure/alert.html.twig' %}
		{% endfor %}
		{% block content %}{% endblock %}

	</div>


{% endblock %}
